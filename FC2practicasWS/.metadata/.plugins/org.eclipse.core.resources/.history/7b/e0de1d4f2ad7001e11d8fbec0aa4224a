.extern _stack
.global main
.equ N, 4

.data
A:      .word 3,5,1,9
B:      .word 1,6,2,3

.bss
	res:  .space 4

.text
main:
	la sp, _stack	//Inicialización de la pila
	la a0, A		//a0 = @base A1
	la a1, A		//a1 = @base A2
	li a2, N		//a2 = N
	call dotprod
	la t0, res		//t0 = @base Res
	sw a0, 0(t0)
fin:
	j fin

dotprod:
	addi sp, sp, -24 	/////
	sw ra, 20( sp) 		//
	sw s0, 16( sp) 		//
	sw s1, 12( sp) 		// PRÓLOGO
	sw s2, 8( sp) 		//
	sw s3, 4( sp) 		//
	sw s4, 0( sp) 		/////
	li s0, 0 			// s0 = acc = 0
	li s1, 0 			// s1 = i
	mv s2, a0 			// s2 = @A1
	mv s3, a1 			// s3 = @A2
	mv s4, a2 			// s4 = N

while:
	bge s1, s4, ewhile


ewhile:






