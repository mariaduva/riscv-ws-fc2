**  Notas de diseño:
**
**	# define N 10
**	int res = 0;
**	for (int i = 0; i < N ; i ++) {
**		res += i ;
**	}
**
**-------------------------------------------------------------------*/
.global main
.equ N, 10 //defino la constante N

.bss
	res: 	.space 4 //reservo espacio para el resultado
//programa
.text
main:
	li s1,n // s1 es n
	li s2,zero // s2 es i
	li s3, zero // s3 es res
for:


efor:
	j .
.end
